{
    "name": "Tag It",
    "version": "0.0.1",
    "description": "Simply store, tag and search web pages of interest",
    "author": "ugo.matrangolo@gmail.com",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "tagit.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+S",
                "mac": "MacCtrl+S"
            }
        }
    }
}
